set nocp
set noswapfile nobk
set ts=4 et sts=2 sw=2
set bs=indent,eol,start
set nojoinspaces gdefault ignorecase
nnoremap  s :update<CR>
silent! syntax on
silent! set wildmenu
silent! set wildignore+=.*
silent! set wildignorecase
silent! set undodir=~/.vim/undo
silent! set clipboard=unnamed
silent! set mouse=a
